{
  "name": "react-group",
  "description": "React component to render collection of items separated by space or other separator",
  "version": "0.0.0-development",
  "homepage": "https://github.com/sapegin/react-group",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sapegin/react-group.git"
  },
  "bugs": {
    "url": "https://github.com/sapegin/react-group/issues"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.4.0",
    "eslint-config-tamia": "^6.0.2",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-styleguidist": "9.0.0-beta4",
    "react-test-renderer": "^16.4.2",
    "webpack": "4.17.1"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest",
    "posttest": "npm run format",
    "test:watch": "jest --watch",
    "lint": "eslint . --cache --fix",
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy": "gh-pages -d docs",
    "gh-pages": "npm run styleguide:build && npm run deploy",
    "format": "prettier --write '**/*.{js,md}'",
    "precommit": "lint-staged"
  },
  "keywords": [
    "react",
    "jsx",
    "group",
    "list",
    "separator",
    "collection",
    "space"
  ],
  "jest": {
    "testURL": "http://localhost/"
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
